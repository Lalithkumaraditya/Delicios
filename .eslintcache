 => {\n        this.setState({ error: error });\n      });\n    }\n    errorConfirmedHandler = () => {\n      this.setState({ error: null });\n    };\n    componentWillUnmount(){\n   axios.interceptors.request.eject(this.reqInterceptor);\n   axios.interceptors.request.eject(this.resInterceptor);\n    }\n    render() {\n      return (\n        <React.Fragment>\n          <Modal show={this.state.error} modalClosed={this.errorConfirmedHandler}>\n            {this.state.error ? this.state.error.message : null }\n          </Modal>\n          <WrappedComponent {...this.props} />\n        </React.Fragment>\n      );\n    }\n  };\n};\nexport default withErrorHandler;\n","/Volumes/Macintosh HD - Data/Profession/Projects/React/Maxmillion/React/Delicious/delicious/src/Components/Order/CheckoutSummary/CheckoutSummary.js",["202"],"/Volumes/Macintosh HD - Data/Profession/Projects/React/Maxmillion/React/Delicious/delicious/src/Containers/Checkout/Checkout.js",[],"/Volumes/Macintosh HD - Data/Profession/Projects/React/Maxmillion/React/Delicious/delicious/src/Containers/ContactData/ContactData.js",["203"],"/Volumes/Macintosh HD - Data/Profession/Projects/React/Maxmillion/React/Delicious/delicious/src/Containers/Orders/Orders.js",["204"],"import React,{ Component } from \"react\";\nimport Order from '../../Components/Order/Order';\nimport axios from '../../axios';\nimport withErrorHandler from '../../hoc/withErrorHandler/withErrorHandler';\nclass Orders extends Component{\n    state={\n        orders:[],\n        loading:true\n    }\n    componentDidMount(){\n      axios.get('/orders.json')\n      .then(res=>{\n          const fetchOrders =[];\n          for(let key in res.data){\n            fetchOrders.push(\n               { \n                ...res.data[key],\n                id : key\n            });\n            console.log(fetchOrders);\n          }\n        this.setState({loading:false,orders:fetchOrders});\n      })\n      .catch(err =>{\n        this.setState({loading:fals