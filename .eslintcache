e({ error: error });\n      });\n    }\n    errorConfirmedHandler = () => {\n      this.setState({ error: null });\n    };\n    componentWillUnmount(){\n   axios.interceptors.request.eject(this.reqInterceptor);\n   axios.interceptors.request.eject(this.resInterceptor);\n    }\n    render() {\n      return (\n        <React.Fragment>\n          <Modal show={this.state.error} modalClosed={this.errorConfirmedHandler}>\n            {this.state.error ? this.state.error.message : null }\n          </Modal>\n          <WrappedComponent {...this.props} />\n        </React.Fragment>\n      );\n    }\n  };\n};\nexport default withErrorHandler;\n",["208","209"],"/Volumes/Macintosh HD - Data/Profession/Projects/React/Maxmillion/React/Delicious/delicious/src/Components/Order/CheckoutSummary/CheckoutSummary.js",[],"/Volumes/Macintosh HD - Data/Profession/Projects/React/Maxmillion/React/Delicious/delicious/src/Containers/Checkout/Checkout.js",[],"/Volumes/Macintosh HD - Data/Profession/Projects/React/Maxmillion/React/Delicious/delicious/src/Containers/ContactData/ContactData.js",["210"],"import React, { Component } from \"react\";\nimport Button from \"../../Components/UI/Button/Button\";\nimport \"./ContactData.css\";\nimport axios from \"../../axios\";\nimport Spinner from \"../../Components/Spinner/Spinner\";\nimport Input from \"../../Components/UI/Input/Input\";\nimport { textAlign } from \"@material-ui/system\";\nimport {connect} from 'react-redux';\nclass ContactData extends Component {\n  state = {\n    orderForm: {\n      name: {\n        elementtype:'input',\n        elementconfig:{\n          type:\"text\",\n          placeholder:\"Enter Your Name\",\n          helper:\"Enter Valid  Name\"\n        },\n        value:'',\n        Validation:{\n          required:true\n        },\n        valid:false,\n        touched:false\n      },\n      street: {\n        elementtype:'input',\n        elementconfig:{\n          type:\"text\",\n          placeholder:\"Enter Your Street